<!-- NAVBAR
================================================== -->

<body>

  <nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
      <div class="navbar-header">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="/">Home</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <font color="black" size="3em">Blueprint 관리자</font>
      </div>

    </div>
    <!--/.navbar-collapse -->
  </nav>


  <!-- Carousel
    ================================================== -->
  <div id="myCarousel" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <div class="carousel-inner" role="listbox">
      <div class="item active">
        <div class="container">
          <div class="carousel-caption">
            <!--받아오기-->
            <h1>
              <%=user_id %> 님 안녕하세요!</h1>
            <p>나의 보유토큰
              <%=user_balance %>
            </p>
            <p>토큰 구매 :
            <form  action = "/buy" method="POST">
            
            <input type="printer_name" name = "pass"  size = "30" placeholder="password" required autofocus> 
            <input type="printer_location" name = "amount"  placeholder="amount" required autofocus>
            <button  type="submit">BUY</button>
            </form>
            </p>
          </div>
        </div>
      </div>
    </div><!-- /.carousel -->


    <!-- Marketing messaging and featurettes
    ================================================== -->
    <!-- Wrap the rest of the page in another container to center all the content. -->

    <div class="container marketing">
      <!-- 세가지 메뉴 출력란-->
      <div class="row">
        <div class="col-lg-4">
          <br>
          <img class="img-circle" src="../image/buy.png" alt="Generic placeholder image" width="100" height="100">
          <br><br>
          <h3>나의 주문내역</h3>
          <p>나의 주문내역을 확인합니다.</p>
          <p><a class="btn btn-default" href="/Log_in/List/Paper" role="button">Paper Order View details &raquo;</a></p>
          <p><a class="btn btn-default" href="/Log_in/List/Toner" role="button">Toner Order View details &raquo;</a></p>
          <p><a class="btn btn-default" href="/Log_in/List/Repair" role="button">Repair Order View details &raquo;</a></p>
        </div><!-- /.col-lg-4 -->
        <div class="col-lg-4">
          <br>
          <img class="img-circle" src="../image/printer.png" alt="Generic placeholder image" width="100" height="100">
          <br><br>
          <h2>프린터 등록</h2>
          <p>프린터를 등록합니다.</p>
          <p><a class="btn btn-default" href="/Log_in/Register" role="button">View details &raquo;</a></p>
        </div><!-- /.col-lg-4 -->
        <div class="col-lg-4">
          <br>
          <img class="img-circle" src="../image/manage.png" alt="Generic placeholder image" width="100" height="100">
          <br><br>
          <h2>프린터 관리</h2>
          <p>등록된 프린터를 조회하거나 삭제합니다.</p>
          <p><a class="btn btn-default" href="/Log_in/Manage" role="button">View details &raquo;</a></p>
        </div><!-- /.col-lg-4 -->
      </div><!-- /.row -->


    </div><!-- /.container -->
